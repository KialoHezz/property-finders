<% properties.each do |property| %>
    <div class="col-4">
        <div class="property border">
            <%= link_to property_path(property) do%>
                <div class="property-thumb" >
                    <%= link_to(property_thumbnail_url(property), property_path(property))%>
                </div>

                <div class="property-summary p-3">
                    <h3 class="h5"><%= link_to property.name,  property_path(property), class:"text-dark" %></h3>
                    <span class="mr-2 text-muted ml-1"><i data-feather="u">bed</i></span> <%= property.rooms %>
                    <span class="mr-2 text-muted ml-1"><i data-feather="fe:car">bath</i></span> <%= property.bathrooms %>
                    <span class="mr-2 text-muted ml-1"><i data-feather="fe:car">car</i></span> <%= property.parking_spaces %>
                    
                    <p class="mt-2" ><%= property.address %></p>
                </div>
            <% end %>
        </div>
    </div>
<% end %>



            